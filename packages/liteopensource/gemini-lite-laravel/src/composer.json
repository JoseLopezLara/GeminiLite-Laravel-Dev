{
    "name": "liteopensource/gemini-lite-laravel",
    "description": "Redisingned and easy to use Gemini Lite SDK for Laravel",
    "type": "library",
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/support": "^9.0 || ^10.0 || ^11.0",
        "illuminate/console": "^9.0 || ^10.0 || ^11.0",
        "illuminate/database": "^9.0 || ^10.0 || ^11.0"
    },
    "authors": [
        {
            "name": "José López Lara",
            "email": "jose.lopez.lara.cto@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "LiteOpenSource\\GeminiLiteLaravel\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LiteOpenSource\\GeminiLiteLaravell\\Providers\\GeminiLiteServiceProvider"
            ],
            "aliases": {
                "UploadFileToGemini": "LiteOpenSource\\GeminiLiteLaravel\\Facades\\UploadFileToGemini"
            }
        }
    },
    "minimum-stability": "dev"
}
